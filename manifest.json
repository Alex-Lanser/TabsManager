{
    "manifest_version": 3,
    "name": "Tab Manager",
    "version": "2.0",
    "icons": {
        "16": "tabs16.png",
        "32": "tabs32.png",
        "128": "tabs128.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ],
    "permissions": [
        "tabGroups",
        "tabs",
        "storage"
    ]
}